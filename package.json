{
  "name": "@antv/infographic-ssr",
  "version": "0.2.0",
  "description": "Server-side rendering for @antv/infographic",
  "keywords": ["antv", "infographic", "ssr", "svg", "server-side-rendering"],
  "repository": "git@github.com:antvis/infographic-ssr.git",
  "license": "MIT",
  "author": "AntV",
  "bin": {
    "infographic-ssr": "./lib/cli/index.js"
  },
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "import": "./esm/index.js",
      "require": "./lib/index.js"
    }
  },
  "files": ["lib", "esm", "src"],
  "scripts": {
    "build": "run-s clean build:esm build:cjs",
    "build:esm": "tsc --module ESNext --outDir esm",
    "build:cjs": "tsc --module CommonJS --outDir lib",
    "clean": "rimraf esm lib",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write .",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:update": "vitest --run --update",
    "prepublishOnly": "npm run build && npm test"
  },
  "peerDependencies": {
    "@antv/infographic": ">=0.2.0"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "linkedom": "^0.18.0",
    "svgo": "^3.2.0"
  },
  "devDependencies": {
    "@antv/infographic": "^0.2.0",
    "@types/node": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "rimraf": "^6.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
